<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.caloriesdiary">

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.caloriesdiary.MainActivity"
            android:theme="@style/AppTheme.FullScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.caloriesdiary.pageListView"
            android:theme="@style/AppTheme.NoActionBar"/>
        <activity android:name="com.example.caloriesdiary.pageAdd1"
            android:theme="@style/AppTheme.NoActionBar"/>
        <activity android:name="com.example.caloriesdiary.pageChoose"
            android:theme="@style/AppTheme.NoActionBar"/>
        <activity android:name="com.example.caloriesdiary.pageAdd2"
            android:theme="@style/AppTheme.NoActionBar"/>
        <activity android:name="com.example.caloriesdiary.pageAdd3"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>